// LEET CODE PROBLEM 2778 : https://leetcode.com/problems/sum-of-squares-of-special-elements

var sumOfSquares = function(nums) {
    var res = 0;
    for(i=1;i<=nums.length;i++)
    {
        if(nums.length % i == 0)
        {
            res += nums[i-1]*nums[i-1];
        }
    }
    return res;
};
