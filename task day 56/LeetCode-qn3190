// LEET CODE PROBLEM 3190 : https://leetcode.com/problems/find-minimum-operations-to-make-all-elements-divisible-by-three

Using JavaScript

var minimumOperations = function(nums) { 
    let count = 0;
    for(i=0;i<nums.length;i++)
    {
        if(nums[i]%3!=0)
        {
            count++;
        }
    }
    return count;
};

Using C#

public class Solution {
    public int MinimumOperations(int[] nums) {
        int count = 0;
        for(int i=0;i<nums.Length;i++)
        {
            if(nums[i]%3!=0)
            {
                count++;
            }
        }
        return count;
    }
}
